color-secondary = rgba(0, 0, 0, .43)

.UserProject
  &:not(:first-child)
    border-top: 1px solid rgba(23, 77, 161, 0.43)
    margin-top: 20px
    padding-top: 20px

.UserProjectContainer
  margin-top: 12px

.UserProjectDate
  color: color-secondary
  font-size: 12px

.UserProjectHeader
  display: flex

.UserProjectInfo
  flex: 1 1 auto
  margin-left: 12px

.UserProjectLogo
  display: 0 0 36px
  height: 36px
  width: 36px

.UserProjectTitle
  font-size: 18px
